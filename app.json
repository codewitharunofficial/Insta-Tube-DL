{
  "expo": {
    "name": "Insta-Tube-DL",
    "slug": "Insta-Tube-DL",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "instatubedl",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arunonwork.InstaTubeDL",
      "associatedDomains": [
        "applinks:youtube.com",
        "applinks:www.youtube.com",
        "applinks:instagram.com",
        "applinks:www.instagram.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.arunonwork.InstaTubeDL",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "youtube.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "www.youtube.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "youtu.be",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "instagram.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "https",
              "host": "www.instagram.com",
              "pathPrefix": "/"
            },
            {
              "scheme": "instatubedl"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "autoVerify": true
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": {
            "mimeType": "text/plain"
          }
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5bb40107-e20d-4df8-914f-9aedbb361dbb"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/5bb40107-e20d-4df8-914f-9aedbb361dbb"
    }
  }
}
